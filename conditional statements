// Grading Plan
// 100 - 80 : "A";
// 80 - 60 : "B";
// 60 - 40 : "C";
// 40 - 10 : "Fail"



const marks = 40;
switch (marks) {
    case 100 > marks && marks > 80:
        console.log("A");
        break;
    case 80 >= marks && marks > 60:
        console.log("B");
        break;
    case 60 > marks && marks >= 40:
        console.log("C");
        break;
    case 40 > marks && marks > 0:
        console.log("D");
        break;
    default:
        console.log("Provide valid marks");
        break;
}




